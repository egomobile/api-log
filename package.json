{
  "name": "@egomobile/api-log",
  "version": "0.4.0",
  "description": "Extension for js-log to store in logs-service",
  "main": "lib/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "del ./lib && tsc",
    "docs:generate": "del ./docs && typedoc --out ./docs ./src",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
    "sandbox": "nodemon --watch './sandbox/index.ts' ./sandbox/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/egomobile/api-log"
  },
  "author": "Next.e.GO Mobile SE, Aachen, Germany",
  "license": "LGPL-3.0",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "lib",
    "!lib/**/*.map",
    "README.md"
  ],
  "dependencies": {
    "axios": "1.3.4",
    "joi": "17.8.3",
    "stack-trace": "^1.0.0-pre2"
  },
  "devDependencies": {
    "@egomobile/log": "0.3.0",
    "@egomobile/tsconfig": "^5.0.0",
    "@types/stack-trace": "^0.0.30",
    "del-cli": "5.0.0",
    "dotenv": "16.0.3",
    "eslint": "8.35.0",
    "eslint-config-ego": "^0.17.1",
    "nodemon": "2.0.20",
    "ts-node": "10.9.1",
    "typedoc": "0.23.26",
    "typescript": "4.6.3"
  },
  "peerDependencies": {
    "@egomobile/log": "0.3.0"
  }
}
