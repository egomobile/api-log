{
  "name": "@egomobile/api-log",
  "version": "0.1.1",
  "description": "Extension for js-log to store in logs-service",
  "main": "lib/index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "del ./lib && tsc",
    "docs:generate": "del ./docs && typedoc --out ./docs ./src",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
    "sandbox": "nodemon --watch './sandbox/index.ts' ./sandbox/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/egomobile/api-log"
  },
  "author": "Next.e.GO Mobile SE, Aachen, Germany",
  "license": "LGPL-3.0",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "lib",
    "!lib/**/*.map",
    "README.md"
  ],
  "dependencies": {
    "@types/node": "12.20.27",
    "axios": "0.24.0",
    "joi": "17.5.0"
  },
  "devDependencies": {
    "@egomobile/log": "0.3.0",
    "@egomobile/tsconfig": "5.0.0",
    "del-cli": "4.0.1",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-ego": "0.15.0",
    "nodemon": "2.0.13",
    "ts-node": "10.2.1",
    "tslint": "6.1.3",
    "typedoc": "0.22.5",
    "typescript": "4.4.3"
  },
  "peerDependencies": {
    "@egomobile/log": "0.3.0"
  }
}